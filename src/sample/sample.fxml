<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="400.0" maxWidth="500.0" minHeight="400.0" minWidth="500.0" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane fx:id="gameField" alignment="CENTER" gridLinesVisible="true" maxHeight="300.0" maxWidth="300.0" minHeight="300.0" minWidth="300.0" onMouseClicked="#fieldClicked" prefHeight="300.0" prefWidth="300.0">
         <rowConstraints>
            <RowConstraints maxHeight="100.0" minHeight="100.0" prefHeight="100.0" />
            <RowConstraints maxHeight="100.0" minHeight="100.0" prefHeight="100.0" />
            <RowConstraints maxHeight="100.0" minHeight="100.0" prefHeight="100.0" />
         </rowConstraints>
         <columnConstraints>
            <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
            <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
            <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="50.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Pane id="0" fx:id="pane00" prefHeight="200.0" prefWidth="200.0" />
            <Pane id="1" fx:id="pane10" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <Pane id="2" fx:id="pane20" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
            <Pane id="3" fx:id="pane01" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
            <Pane id="4" fx:id="pane11" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Pane id="5" fx:id="pane21" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Pane id="6" fx:id="pane02" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
            <Pane id="7" fx:id="pane12" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Pane id="8" fx:id="pane22" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <HBox alignment="CENTER_LEFT" spacing="20.0">
         <children>
            <CheckBox fx:id="circleSwitch" mnemonicParsing="false" onAction="#changeCircleSwitch" text="Circles ?" />
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#startApp" text="Start!" />
            <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stopApp" text="Stop" />
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
